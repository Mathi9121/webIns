{% extends '::app.html.twig' %}

{% block body -%}
    <h1>Inscription</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Dateinscription</th>
                <td>{{ entity.dateInscription|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ entity.statut }}</td>
            </tr>
            <tr>
                <th>Attentes</th>
                <td>{{ entity.attentes }}</td>
            </tr>
            <tr>
                <th>Statutorgfinanceur</th>
                <td>{{ entity.statutOrgFinanceur }}</td>
            </tr>
            <tr>
                <th>Statutconvention</th>
                <td>{{ entity.statutConvention }}</td>
            </tr>
            <tr>
                <th>Hash</th>
                <td>{{ entity.hash }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('inscription', {'id':idformation}) }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('inscription_edit', { 'id': entity.id, 'idformation': idformation }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
